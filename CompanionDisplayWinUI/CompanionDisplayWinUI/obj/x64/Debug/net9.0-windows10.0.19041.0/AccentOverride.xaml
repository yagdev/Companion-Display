    <Color x:Key="SystemAccentColorDark3">#FF0078D7</Color>
    <Color x:Key="SystemAccentColorLight1">#FF0078D7</Color>
    <Color x:Key="SystemAccentColorLight2">#FF0078D7</Color>
    <Color x:Key="SystemAccentColorLight3">#FF0078D7</Color>
